<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>Cratis.Events.Store.Grpc.Contracts</AssemblyName>
        <RootNamespace>Cratis.Events.Store.Grpc.Contracts</RootNamespace>
        <Nullable>disable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="protobuf-net" Version="$(ProtobufNet)" />
        <PackageReference Include="protobuf-net.BuildTools" Version="$(ProtobufNet)" PrivateAssets="all" IncludeAssets="runtime;build;native;contentfiles;analyzers;buildtransitive" />
        <PackageReference Include="protobuf-net.Grpc" Version="$(ProtobufNetGrpc)" />
        <PackageReference Include="System.ServiceModel.Primitives" Version="$(ServiceModelPrimitives)" />
    </ItemGroup>
</Project>
