<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>Cratis.Extensions.Dolittle</AssemblyName>
        <RootNamespace>Cratis.Extensions.Dolittle</RootNamespace>

        <AksioProxyOutput>$(MSBuildThisFileDirectory)API</AksioProxyOutput>
        <AksioUseRouteAsPath>true</AksioUseRouteAsPath>

    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Newtonsoft.Json.Schema" Version="$(NewtonsoftJsonSchema)" />
        <PackageReference Include="Dolittle.SDK" Version="11.0.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..//MongoDB/MongoDB.csproj" />
        <ProjectReference Include="../../Fundamentals/Fundamentals.csproj"/>
        <ProjectReference Include="../../Clients/DotNET/DotNET.csproj">
            <Aliases>SDK</Aliases>
        </ProjectReference>
        <ProjectReference Include="../../Clients/AspNetCore.Workbench/AspNetCore.Workbench.csproj"/>
    </ItemGroup>
</Project>
