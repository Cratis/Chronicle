{
    "name": "workbench",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint --cache --quiet --fix",
        "lint:ci": "eslint --cache --quiet",
