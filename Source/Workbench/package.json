{
    "name": "workbench",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --cache --quiet --fix --report-unused-disable-directives --max-warnings 0",
        "lint:ci": "eslint . --cache --quiet --ext ts,vue --report-unused-disable-directives --max-warnings 0",
        "test": "mocha --config ./.mocharc.cjs",
        "ci": "yarn clean && yarn lint:ci && tsc -b && yarn test",
        "up": "ncu -u",
        "preview": "vite preview"
    },
    "dependencies": {
        "echarts": "5.5.0",
        "primeicons": "7.0.0",
        "primevue": "4.0.0-beta.1",
        "reflect-metadata": "0.2.2",
        "tsyringe": "4.8.0",
        "vue": "3.4.25"
    },
    "devDependencies": {
        "@eslint/js": "9.1.1",
        "@types/chai": "4.3.14",
        "@types/chai-as-promised": "7.1.8",
        "@types/mocha": "10.0.6",
        "@types/node": "20.12.7",
        "@types/sinon": "17.0.3",
        "@types/sinon-chai": "3.2.12",
        "@vitejs/plugin-vue": "5.0.4",
        "autoprefixer": "10.4.19",
        "chai": "5.1.0",
        "chai-as-promised": "7.1.1",
        "eslint": "9.1.1",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-headers": "1.1.2",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-no-null": "1.0.2",
        "eslint-plugin-prettier": "5.1.3",
        "eslint-plugin-vue": "9.25.0",
        "mocha": "10.4.0",
        "postcss": "8.4.38",
        "sinon": "17.0.1",
        "sinon-chai": "3.7.0",
        "tailwindcss": "3.4.3",
        "ts-node": "10.9.2",
        "typescript": "5.4.5",
        "typescript-eslint": "7.7.1",
        "vite": "5.2.10",
        "vue-eslint-parser": "9.4.2",
        "vue-tsc": "2.0.14"
    }
}
