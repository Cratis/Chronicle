{
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
        ],
        "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
                "Aksio": "Information",
                "Microsoft": "Warning",
                "Microsoft.AspNetCore.HttpLogging": "Information",
                "Microsoft.Hosting.Lifetime": "Information",
                "System": "Warning",
                "Orleans": "Information",
                "Websocket": "Information"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    },
    "AllowedHosts": "*"
}
