<Project>
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <NoWarn>$(NoWarn);CS1591;RCS1090;CA2252;CS8632;RCS1217</NoWarn>
        <Nullable>enable</Nullable>
        <TreatWarningsAsErrors>true</TreatWarningsAsErrors>

        <IsPackable>true</IsPackable>
        <Copyright>Copyright Cratis</Copyright>
        <Authors>all contributors</Authors>
        <DebugType>portable</DebugType>
        <PackageIcon>logo.png</PackageIcon>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <RepositoryType>git</RepositoryType>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <RepositoryUrl>https://github.com/Cratis/cratis</RepositoryUrl>
        <PackageProjectUrl>https://github.com/Cratis/cratis</PackageProjectUrl>

        <IncludeSymbols>True</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
        <IncludeSource>True</IncludeSource>

        <GenerateDocumentationFile>true</GenerateDocumentationFile>

        <ImplicitUsings>true</ImplicitUsings>

        <AksioProxyOutput>$(MSBuildThisFileDirectory)Source/Workbench/Events/Web/API</AksioProxyOutput>
        <AksioUseRouteAsPath>true</AksioUseRouteAsPath>
    </PropertyGroup>

    <ItemGroup>
        <Content Include="$(MSBuildThisFileDirectory)logo.png" PackagePath="/" />
    </ItemGroup>

    <!-- <Import Project="$(MSBuildThisFileDirectory)../../Aksio/Foundation/Source/Tooling/ProxyGenerator/build/Aksio.ProxyGenerator.props"/> -->

    <ItemGroup>
        <!-- <ProjectReference Include="$(MSBuildThisFileDirectory)../../Aksio/Foundation/Source/Tooling/ProxyGenerator/ProxyGenerator.csproj" OutputItemType="Analyzer" Private="true"/> -->
        <PackageReference Include="Aksio.ProxyGenerator" Version="$(Aksio)"/>
    </ItemGroup>

    <PropertyGroup>
        <HotChocolateVersion>11.3.0</HotChocolateVersion>
        <NewtonsoftJson>13.0.1</NewtonsoftJson>
        <Autofac>6.2.0</Autofac>
        <AutofacExtensions>7.1.0</AutofacExtensions>
        <Aksio>1.12.6</Aksio>
        <Orleans>3.4.3</Orleans>
        <MongoDB>2.13.1</MongoDB>
        <ProtobufNet>3.0.101</ProtobufNet>
        <ProtobufNetGrpc>1.0.152</ProtobufNetGrpc>
        <ServiceModelPrimitives>4.8.1</ServiceModelPrimitives>
        <Humanizer>2.11.10</Humanizer>
    </PropertyGroup>
</Project>
