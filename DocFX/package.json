{
    "name": "site",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "private": true,
    "scripts": {
        "watch": "rm -rf ./_site && rm -rf ./obj && dotnet clean && dotnet restore && concurrently 'yarn serve' 'npm-watch build'",
        "build": "dotnet msbuild -t:docfx",
        "serve": "yarn http-server ./_site"
    },
    "watch": {
        "build": {
            "patterns": [
                "app",
                "toc",
                "index",
                "../Source",
                "../Documentation"
            ],
            "ignore": [
                "_site"
            ],
            "extensions": "md,yml,cs,ts,tsx",
            "quiet": false
        }
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "concurrently": "7.3.0",
        "http-server": "14.1.1",
        "npm-watch": "0.11.0"
    }
}
