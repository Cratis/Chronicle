{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "**/**.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**",
                        "Tooling/Templates/**"
                    ],
                    "cwd": "../Source/ApplicationModel"
                }
            ],
            "dest": "api/application-model",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        },
        {
            "src": [
                {
                    "files": [
                        "Clients/DotNET/DotNET.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**"
                    ],
                    "cwd": "../Source"
                }
            ],
            "dest": "api/clients/dotnet",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        },
        {
            "src": [
                {
                    "files": [
                        "Clients/AspNetCore/AspNetCore.csproj"
                    ],
                    "exclude": [
                        "**/obj/**",
                        "**/bin/**"
                    ],
                    "cwd": "../Source"
                }
            ],
            "dest": "api/clients/aspnetcore",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }

    ],
    "build": {
        "content": [
            {
                "files": [
                    "api/**.yml",
                    "api/index.md"
                ]
            },
            {
                "files": [
                    "**/*.md",
                    "**/toc.yml"
                ],
                "exclude": [
                    "index.md"
                ],
                "src": "../Documentation",
                "dest": "articles"
            },
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            },
            {
                "files": [
                    "**/*.png",
                    "**/*.gif",
                    "**/*.jpg"
                ],
                "src": "../Documentation",
                "dest": "articles"
            }

        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/singulinkfx"
        ],
        "postProcessors": ["ExtractSearchIndex"],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false,
        "globalMetadata": {
            "_appTitle": "Cratis",
            "_appName": "Cratis",
            "_appFaviconPath": "images/favicon.png",
            "_appLogoPath": "images/logo.png",
            "_appFooter": "Documentation",
            "_copyrightFooter": "Â© Aksio Insurtech. All rights reserved.",
            "_enableSearch": true,
            "_disableSideFilter": false,
            "_enableNewTab": true,
            "_disableContribution": false,
            "_disableBreadcrumb": false
        }
    }
}


